programa {
  funcao inicio() {
   //Calcula a área do paralelograma

   real base
   real altura

   escreva("\nInforme a base do paralelograma: ")
   leia(base)

   escreva("Informe a altura paralelograma: ")
   leia(altura)
   
   escreva("O valor da área do paralelogramo é: ", (base * altura))
  }
}
